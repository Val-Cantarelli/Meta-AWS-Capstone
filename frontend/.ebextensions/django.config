option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: core/wsgi.py

container_commands:
  01_collectstatic:
    command: |
      source /var/app/venv/*/bin/activate
      python manage.py collectstatic --noinput > /tmp/collectstatic.log 2>&1 || cat /tmp/collectstatic.log
    leader_only: true